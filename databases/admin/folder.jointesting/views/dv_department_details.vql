CREATE OR REPLACE VIEW dv_department_details FOLDER = '/jointesting' AS SELECT DISTINCT bv_hr_departments.department_id AS department_id, bv_hr_departments.department_name AS department_name, bv_hr_departments.manager_id AS manager_id, dv_employees_department_manager.manager_first_name AS manager_first_name, dv_employees_department_manager.manager_last_name AS manager_last_name, dv_employees_department_manager.manager_email AS manager_email, bv_hr_departments.location_id AS location_id, bv_hr_locations.street_address AS street_address, bv_hr_locations.postal_code AS postal_code, bv_hr_locations.city AS city, bv_hr_locations.state_province AS state_province, bv_hr_countries.country_name AS country_name, bv_hr_regions.region_name AS region_name FROM bv_hr_regions AS bv_hr_regions INNER JOIN (((bv_hr_departments AS bv_hr_departments INNER JOIN dv_employees_department_manager_testing AS dv_employees_department_manager ON bv_hr_departments.department_id = dv_employees_department_manager.department_id ) INNER JOIN bv_hr_locations AS bv_hr_locations ON bv_hr_departments.location_id = bv_hr_locations.location_id ) INNER JOIN bv_hr_countries AS bv_hr_countries ON bv_hr_locations.country_id = bv_hr_countries.country_id ) ON bv_hr_regions.region_id = bv_hr_countries.region_id ;

ALTER VIEW dv_department_details
 LAYOUT (bv_hr_regions = [600, 580, 200, 66], bv_hr_departments = [50, 210, 200, 106], dv_employees_department_manager_testing = [280, 480, 200, 225], bv_hr_locations = [510, 200, 200, 146], bv_hr_countries = [910, 310, 200, 86]);

